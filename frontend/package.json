  "scripts": {
    "build": "gatsby clean && GATSBY_EXPERIMENTAL_FORCE_FS_STORE=true gatsby build",
    "build:vercel": "gatsby clean && GATSBY_EXPERIMENTAL_FORCE_FS_STORE=true gatsby build",
